{
  "expo": {
    "name": "Dumax-ELD-App",
    "slug": "Dumax-ELD-App",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "@config-plugins/react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ],
          "bluetoothAlwaysPermission": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ],
      "expo-updates",
      {
        "username": "dguzmandumax"
      }
    ],
    "extra": {
      "eas": {
        "projectId": "8ba5bb38-abbc-41a1-abd1-a87720b682ab"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/8ba5bb38-abbc-41a1-abd1-a87720b682ab"
    },
    "android": {
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ]
    },
    "ios": {}
  }
}
